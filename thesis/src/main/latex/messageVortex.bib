@Article{LEMIRE2019139,
  author    = {Lemire, Daniel and O'Neill, Melissa E.},
  title     = {Xorshift1024*, xorshift1024+, xorshift128+ and xoroshiro128+ fail statistical tests for linearity},
  journal   = {Journal of Computational and Applied Mathematics},
  year      = {2019},
  volume    = {350},
  pages     = {139 - 142},
  issn      = {0377-0427},
  abstract  = {Lâ€™Ecuyer & Simardâ€™s BigÂ Crush statistical test suite has revealed statistical flaws in many popular random number generators including Marsagliaâ€™s Xorshift generators. Vigna recently proposed some 64-bit variations on the Xorshift scheme that are further scrambled (i.e., xorshift1024*, xorshift1024+, xorshift128+, xoroshiro128+). Unlike their unscrambled counterparts, they pass BigÂ Crush when interleaving blocks of 32Â bits for each 64-bit word (most significant, least significant, most significant, least significant, etc.). We report that these scrambled generators systematically fail Big Crush â€“ specifically the linear-complexity and matrix-rank tests that detect linearity â€“ when taking the 32Â lowest-order bits in reverse order from each 64-bit word.},
