@Inproceedings{blacr-ndss,
  Title                    = {{BLACR}: {TTP}-Free Blacklistable Anonymous Credentials with Reputation},
  Author                   = {Man Ho Au and Apu Kapadia and Willy Susilo},
  Booktitle                = {Proceedings of the 19th Annual Network and Distributed System Security Symposium ({NDSS})},
  Year                     = {2012},
  Month                    = feb,
  Url                      = {http://www.cs.indiana.edu/~kapadia/papers/blacr-ndss-draft.pdf},

  Abstract                 = {Anonymous authentication can give users the license to misbehave since there is no fear of retribution. As a deterrent, or means to revocation, various schemes for accountable anonymity feature some kind of (possibly distributed) trusted third party (TTP) with the power to identify or link misbehaving users. Recently, schemes such as BLAC and PEREA showed how anonymous revocation can be achieved without such TTPs‚Äîanonymous users can be revoked if they misbehave, and yet nobody can identify or link such users cryptographically. Despite being the state of the art in anonymous revocation, these schemes allow only a basic form of revocation amounting to ‚Äòrevoke anybody with d or more misbehaviors‚Äô or ‚Äòrevoke anybody whose combined misbehavior score is too high‚Äô (where misbehaviors are assigned a ‚Äòseverity‚Äô score). We present BLACR, which significantly advances anonymous revocation in three ways: 1) It constitutes a first attempt to generalize reputation-based anonymous revocation, where negative or positive scores can be assigned to anonymous sessions across multiple categories. Servers can block users based on policies, which specify a boolean combination of reputations in these categories; 2) We present a weighted extension, which allows the total severity score to ramp up for multiple misbehaviors by the same user; and, 3) We make a significant improvement in authentication times through a technique we call express lane authentication, which makes reputation-based anonymous revocation practical.},
  File                     = {blacr-ndss.pdf:blacr-ndss.pdf:PDF;:http\://www.cs.indiana.edu/~kapadia/papers/blacr-ndss-draft.pdf:URL;blacr-ndss.pdf::Djvu},
  Lastaccessed             = {2012-04-19 17:57:50},
  Ranking                  = {rank3}
}

